POST {{baseUrl}}{{apiPath}}/search
Content-Type: application/json

{
  "query": "senior software engineer",
  "location": "San Francisco",
  "filters": {
    "experience_level": "senior",
    "min_salary": 150000,
    "skills": ["Python", "Java", "Kubernetes"]
  }
}

